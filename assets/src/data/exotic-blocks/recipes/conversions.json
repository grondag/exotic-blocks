<@pp.dropOutputFile />
<#list conversions as conversion>
<@pp.changeOutputFile name=conversion.from_name + "_to_" + conversion.to_name + ".json" />
{
  "type": "minecraft:stonecutting",
  "ingredient": {
    "item": "${conversion.from_mod}:${conversion.from_name}"
  },
  "result": "${conversion.to_mod}:${conversion.to_name}",
  "count": 1
}
<@pp.changeOutputFile name=conversion.to_name + "_to_" + conversion.from_name + ".json" />
{
  "type": "minecraft:stonecutting",
  "ingredient": {
    "item": "${conversion.to_mod}:${conversion.to_name}"
  },
  "result": "${conversion.from_mod}:${conversion.from_name}",
  "count": 1
}
</#list>
